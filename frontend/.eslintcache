Effect } from \"react\";\nimport { Link } from \"react-router-dom\";\nimport { Button, Row, Col, ListGroup, Image, Card } from \"react-bootstrap\";\nimport { useDispatch, useSelector } from \"react-redux\";\nimport Message from \"../Components/Message\";\nimport Loader from \"../Components/Loader\";\nimport { getOrderDetails } from \"../actions/orderActions\";\nimport Layout from \"../Components/Layout\";\n\nconst OrderScreen = ({ match }) => {\n\n    const orderId = match.params.id;\n    const dispatch = useDispatch();\n\n    \n\n    const orderDetails = useSelector((state) => state.orderDetails );\n    const { order, loading, error } = orderDetails;\n    \n    if(!loading) {\n        // Calculate Prices\n        const addDecimal = (num) => {\n            return (Math.round(num * 100) / 100).toFixed(2);\n        };\n        order.itemsPrice = addDecimal(\n            order.orderItems.reduce((acc, item) => acc + item.price * item.qty, 0)\n        );\n    }    \n\n  useEffect(() => {\n    dispatch(getOrderDetails(orderId));\n    \n  }, []);\n\n  \n  return loading ? <Loader /> : error ? <Message variant=\"danger\">{error}</Message> : (\n    <Layout>\n        <h1>Order {order._id}</h1>\n      <Row>\n        <Col md={8}>\n          <ListGroup variant=\"flush\">\n            <ListGroup.Item>\n              <h2>Shipping</h2>\n              <p>\n                <strong>Address:</strong>\n                {order.shippingAddress.address}, {order.shippingAddress.city}{\" \"}\n                {order.shippingAddress.postalCode},{\" \"}\n                {order.shippingAddress.country}\n              </p>\n              {order.isDelivered ? <Message variant=\"success\">Paid on {order.deliveredAt}</Message> : <Message variant=\"danger\">Not Paid</Message>}\n            </ListGroup.Item>\n            <ListGroup.Item>\n              <h2>Payment Method</h2>\n              <p>\n                <strong>Method: </strong>\n                {order.paymentMethod}\n              </p>\n              {order.isPaid ? <Message variant=\"success\">Paid on {order.paidAt}</Message> : <Message variant=\"danger\">Not Paid</Message>}\n\n            </ListGroup.Item>\n            <ListGroup.Item>\n              <h2>Order Items</h2>\n              {order.orderItems.length === 0 ? (\n                <Message variant=\"info\">Cart is empty</Message>\n              ) : (\n                <ListGroup variant=\"flush\">\n                  {order.orderItems.map((item, ind) => (\n                    <ListGroup.Item key={ind}>\n                      <Row>\n                        <Col md={1}>\n                          <Image\n                            src={item.image}\n                            alt={item.name}\n                            fluid\n                            rounded\n                          />\n                        </Col>\n                        <Col>\n                          <Link to={`/product/${item.product}`}>\n                            {item.name}\n                          </Link>\n                        </Col>\n                        <Col md={4}>\n                          {item.qty} x ${item.price} = ${item.price * item.qty}\n                        </Col>\n                      </Row>\n                    </ListGroup.Item>\n                  ))}\n                </ListGroup>\n              )}\n            </ListGroup.Item>\n          </ListGroup>\n        </Col>\n        <Col md={4}>\n          <Card>\n            <ListGroup variant=\"flush\">\n              <ListGroup.Item>\n                <h2>Order Summary</h2>\n              </ListGroup.Item>\n              <ListGroup.Item>\n                <Row>\n                  <Col>Items</Col>\n                  <Col>${order.itemsPrice}</Col>\n                </Row>\n              </ListGroup.Item>\n              <ListGroup.Item>\n                <Row>\n                  <Col>Shipping</Col>\n                  <Col>${order.shippingPrice}</Col>\n                </Row>\n              </ListGroup.Item>\n              <ListGroup.Item>\n                <Row>\n                  <Col>Tax</Col>\n                  <Col>${order.taxPrice}</Col>\n                </Row>\n              </ListGroup.Item>\n              <ListGroup.Item>\n                <Row>\n                  <Col>Total</Col>\n                  <Col>${order.totalPrice}</Col>\n                </Row>\n              </ListGroup.Item>\n            </ListGroup>\n          </Card>\n        </Col>\n      </Row>\n      \n    </Layout>\n    )\n};\n\nexport default OrderScreen;\n","/home/abdrafay/Work/ProShop/frontend/src/Components/Paginate.jsx",[],"/home/abdrafay/Work/ProShop/frontend/src/Screens/ProductListScreen.jsx",[],"/home/abdrafay/Work/ProShop/frontend/src/Screens/ProductEditScreen.jsx",[],"/home/abdrafay/Work/ProShop/frontend/src/Screens/OrderListScreen.jsx",[],{"ruleId":"265","replacedBy":"266"},{"ruleId":"267","replacedBy":"268"},{"ruleId":"269","severity":1,"message":"270","line":1,"column":17,"nodeType":"271","messageId":"272","endLine":1,"endColumn":26},{"ruleId":"269","severity":1,"message":"273","line":2,"column":42,"nodeType":"271","messageId":"272","endLine":2,"endColumn":53},{"ruleId":"269","severity":1,"message":"274","line":3,"column":10,"nodeType":"271","messageId":"272","endLine":3,"endColumn":19},{"ruleId":"269","severity":1,"message":"270","line":1,"column":17,"nodeType":"271","messageId":"272","endLine":1,"endColumn":26},{"ruleId":"269","severity":1,"message":"275","line":8,"column":9,"nodeType":"271","messageId":"272","endLine":8,"endColumn":17},{"ruleId":"269","severity":1,"message":"276","line":9,"column":3,"nodeType":"271","messageId":"272","endLine":9,"endColumn":12},{"ruleId":"269","severity":1,"message":"277","line":10,"column":3,"nodeType":"271","messageId":"272","endLine":10,"endColumn":7},{"ruleId":"269","severity":1,"message":"278","line":15,"column":11,"nodeType":"271","messageId":"272","endLine":15,"endColumn":18},{"ruleId":"269","severity":1,"message":"279","line":15,"