\r\n  Form,\r\n} from \"react-bootstrap\";\r\nimport { listProductDetails } from \"../actions/productActions\";\r\nimport Loader from \"../Components/Loader\";\r\nimport Message from \"../Components/Message\";\r\n\r\nconst ProductScreen = ({ match, history }) => {\r\n  const [qty, setQty] = useState(1);\r\n  const dispatch = useDispatch();\r\n  const productDetails = useSelector((state) => state.productDetails);\r\n  const { loading, error, product } = productDetails;\r\n  useEffect(() => {\r\n    dispatch(listProductDetails(match.params.id));\r\n  }, [dispatch, match]);\r\n  const addToCartHandler = () => {\r\n    history.push(`/cart/${match.params.id}?qty=${qty}`);\r\n  };\r\n\r\n  const designPage = () => {\r\n    history.push(`/design/${match.params.id}?qty=${qty}`);\r\n  };\r\n  return (\r\n    <>\r\n      <Link className=\"btn btn-light my-3\" to=\"/\">\r\n        Go Back\r\n      </Link>\r\n      {loading ? (\r\n        <Loader />\r\n      ) : error ? (\r\n        <Message variant=\"danger\">{error}</Message>\r\n      ) : (\r\n        <Row>\r\n          <Col md={6}>\r\n            <Image src={product.image} alt={product.name} fluid />\r\n          </Col>\r\n          <Col md={3}>\r\n            <ListGroup variant=\"flush\">\r\n              <ListGroup.Item>\r\n                <h3>{product.name}</h3>\r\n              </ListGroup.Item>\r\n              <ListGroup.Item>\r\n                <Rating\r\n                  value={product.rating}\r\n                  text={`${product.numReviews} reviews`}\r\n                />\r\n              </ListGroup.Item>\r\n              <ListGroup.Item>Price: ${product.price}</ListGroup.Item>\r\n              <ListGroup.Item>\r\n                Description: ${product.description}\r\n              </ListGroup.Item>\r\n            </ListGroup>\r\n          </Col>\r\n          <Col md={3}>\r\n            <Card>\r\n              <ListGroup variant=\"flush\">\r\n                <ListGroup.Item>\r\n                  <Row>\r\n                    <Col>Price:</Col>\r\n                    <Col>\r\n                      <strong>${product.price}</strong>\r\n                    </Col>\r\n                  </Row>\r\n                </ListGroup.Item>\r\n                <ListGroup.Item>\r\n                  <Row>\r\n                    <Col>Status:</Col>\r\n                    <Col>\r\n                      {product.countInStock > 0 ? \"In Stock\" : \"Out of Stock\"}\r\n                    </Col>\r\n                  </Row>\r\n                </ListGroup.Item>\r\n                {product.countInStock > 0 && (\r\n                  <ListGroup.Item>\r\n                    <Row>\r\n                      <Col>Qty</Col>\r\n                      <Col>\r\n                        <Form.Control\r\n                          as=\"select\"\r\n                          value={qty}\r\n                          onChange={(e) => setQty(e.target.value)}\r\n                        >\r\n                          {[...Array(product.countInStock).keys()].map((x) => (\r\n                            <option key={x + 1} value={x + 1}>\r\n                              {x + 1}\r\n                            </option>\r\n                          ))}\r\n                        </Form.Control>\r\n                      </Col>\r\n                    </Row>\r\n                  </ListGroup.Item>\r\n                )}\r\n                <ListGroup.Item>\r\n                  {/* Button to Going to Cart Page and then adding to Cart */}\r\n                  {/* <Button\r\n                    className=\"btn-block\"\r\n                    type=\"button\"\r\n                    onClick={addToCartHandler}\r\n                    disabled={product.countInStock === 0}\r\n                  >\r\n                    Add To Cart\r\n                  </Button> */}\r\n                  {/* Button to Going to Design Page and then adding to Cart */}\r\n                  <Button\r\n                    className=\"btn-block\"\r\n                    type=\"button\"\r\n                    onClick={designPage}\r\n                    disabled={product.countInStock === 0}\r\n                  >\r\n                    Add To Cart\r\n                  </Button>\r\n                </ListGroup.Item>\r\n              </ListGroup>\r\n            </Card>\r\n          </Col>\r\n        </Row>\r\n      )}\r\n    </>\r\n  );\r\n};\r\n\r\nexport default ProductScreen;\r\n","/home/abdrafay/Work/ProShop/frontend/src/Screens/HomeScreen.jsx",[],"/home/abdrafay/Work/ProShop/frontend/src/Screens/LoginScreen.jsx",[],"/home/abdrafay/Work/ProShop/frontend/src/Screens/ProfileScreen.jsx",[],"/home/abdrafay/Work/ProShop/frontend/src/Screens/CartScreen.jsx",[],"/home/abdrafay/Work/ProShop/frontend/src/Screens/RegisterScreen.jsx",[],"/home/abdrafay/Work/ProShop/frontend/src/Screens/PaymentScreen.jsx",[],"/home/abdrafay/Work/ProShop/frontend/src/Screens/ShippingScreen.jsx",[],"/home/abdrafay/Work/ProShop/frontend/src/Screens/PlaceOrderScreen.jsx",[],"/home/abdrafay/Work/ProShop/frontend/src/constants/orderConstants.js",[],"/home/abdrafay/Work/ProShop/frontend/src/constants/productConstants.js",[],"/home/abdrafay/Work/ProShop/frontend/src/constants/userConstants.js",[],"/home/abdrafay/Work/ProShop/frontend/src/constants/cartConstants.js",[],"/home/abdrafay/Work/ProShop/frontend/src/actions/productActions.js",[],"/home/abdrafay/Work/ProShop/frontend/src/actions/cartActions.js",[],"/home/abdrafay/Work/ProShop/frontend/src/actions/userActions.js",[],"/home/abdrafay/Work/ProShop/frontend/src/actions/orderActions.js",[],"/home/abdrafay/Work/ProShop/frontend/src/Components/Rating.jsx",[],"/home/abdrafay/Work/ProShop/frontend/src/Components/Loader.jsx",[],"/home/abdrafay/Work/ProShop/frontend/src/Components/Message.jsx",[],"/home/abdrafay/Work/ProShop/frontend/src/Components/FormContainer.jsx",[],"/home/abdrafay/Work/ProShop/frontend/src/Components/Product.jsx",[],"/home/abdrafay/Work/ProShop/frontend/src/Components/CheckoutSteps.jsx",[],"/home/abdrafay/